{
    "name": "zedsh/tower",
    "type": "library",
    "description": "Lite admin panel from laravel",
    "keywords": [
        "tower"
    ],
    "homepage": "https://github.com/zedsh/tower",
    "license": "MIT",
    "authors": [
        {
            "name": "Zaboev Eduard",
            "email": "zedsh@icloud.com",
            "homepage": "https://zedsh.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "zedsh\\tower\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zedsh\\tower\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "zedsh\\tower\\towerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
